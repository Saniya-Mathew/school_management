<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
     <record id="academic_year_view_list" model="ir.ui.view">
        <field name="name">Academic Year</field>
        <field name="model">academic.year</field>
        <field name ="arch" type="xml">
            <list editable="bottom">
                <field name = "name"/>
            </list>
        </field>
</record>
    <record id="school_academic_year_action" model="ir.actions.act_window">
        <field name="name">Manage Academic Year</field>
        <field name="res_model">academic.year</field>
        <field name="view_mode">list</field>
    </record>
</odoo>