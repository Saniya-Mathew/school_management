<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="template_id" name="Calculations">
        <t t-call="website.layout">
            <div class="container">
                <h1>Calculations</h1>
                <div t-foreach="calculations" t-as="calc">
                    <p t-field="calc.value"/>  <!-- Adjust 'value' to match your model's field -->
                </div>
            </div>
        </t>
    </template>
</odoo>
